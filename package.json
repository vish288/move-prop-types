{
  "name": "move-prop-types",
  "version": "0.8.3",
  "description": "\"Help quickly replace the prop type checker in older react projects to map to prop-types\"",
  "main": "build/mpt.js",
  "types": "build/index.d.ts",
  "type": "module",
  "keywords": [
    "npm",
    "prop-types",
    "react",
    "prop to own",
    "discover",
    "updates",
    "upgrades",
    "dependencies",
    "package.json",
    "updater",
    "version",
    "management",
    "mpt"
  ],
  "directories": {
    "test": "test"
  },
  "bin": {
    "move-prop-types": "./build/mpt.js",
    "mpt": "./build/mpt.js"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test-blob": "node build/mpt.js -F test",
    "build": "tsc && rollup -c",
    "postbuild": "node build/updateFile.js build/mpt.js",
    "dev": "nodemon src/core.ts",
    "patch-publish": "pnpm version patch && pnpm publish",
    "tag": "git push --follow-tags",
    "tag-release": "pnpm run patch-publish && pnpm run tag",
    "lint": "eslint src --ext .ts --fix",
    "lint:check": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"*.ts\"",
    "clean": "rimraf *.log build coverage dist",
    "prepare": "husky",
    "typecheck": "tsc --noEmit"
  },
  "author": "Visweshwaran S",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^14.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^22.10.2",
    "@types/prop-types": "^15.7.15",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "nodemon": "^3.1.10",
    "prettier": "^3.4.2",
    "prop-types": "^15.8.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.45.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": "https://github.com/vish288/move-prop-types.git"
}
