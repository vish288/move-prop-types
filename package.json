{
  "name": "move-prop-types",
  "version": "1.0.0",
  "description": "\"Help quickly replace the prop type checker in older react projects to map to prop-types\"",
  "main": "build/mpt.js",
  "types": "build/index.d.ts",
  "type": "module",
  "keywords": [
    "npm",
    "prop-types",
    "react",
    "prop to own",
    "discover",
    "updates",
    "upgrades",
    "dependencies",
    "package.json",
    "updater",
    "version",
    "management",
    "mpt"
  ],
  "directories": {
    "test": "test"
  },
  "bin": {
    "move-prop-types": "./build/mpt.js",
    "mpt": "./build/mpt.js"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:ci": "vitest run",
    "verify": "node verify/verify-transformation.js",
    "test-blob": "node build/mpt.js -F test",
    "monitor": "node scripts/monitor-workflows.js",
    "monitor:watch": "node scripts/monitor-workflows.js --watch",
    "monitor:latest": "node scripts/monitor-workflows.js --latest",
    "monitor:trigger": "node scripts/monitor-workflows.js --trigger --watch",
    "publish:missing": "node scripts/publish-missing-versions.js",
    "publish:missing:dry": "node scripts/publish-missing-versions.js --dry-run",
    "build": "tsc && rollup -c",
    "postbuild": "node build/updateFile.js build/mpt.js",
    "dev": "nodemon src/core.ts",
    "watch": "tsc --watch",
    "lint": "eslint src --ext .ts --fix",
    "lint:check": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"*.ts\"",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf *.log build coverage dist",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build && pnpm run test:ci && pnpm run lint:check",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:all": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch",
    "publish:npm": "np --no-2fa",
    "publish:ci": "pnpm publish --access public",
    "prerelease": "pnpm run build && pnpm run test:ci",
    "postrelease": "git push --follow-tags origin main"
  },
  "author": "Visweshwaran S",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^14.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/generator": "^7.28.0",
    "@babel/parser": "^7.28.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/traverse": "^7.28.0",
    "@babel/types": "^7.28.1",
    "@eslint/js": "^9.17.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.20.7",
    "@types/node": "^22.10.2",
    "@types/prop-types": "^15.7.15",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "@vitest/ui": "^2.1.8",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.17.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "nodemon": "^3.1.10",
    "np": "^10.2.0",
    "prettier": "^3.4.2",
    "prop-types": "^15.8.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.45.1",
    "semantic-release": "^24.2.7",
    "standard-version": "^9.5.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.9"
  },
  "pnpm": {
    "overrides": {
      "esbuild": ">=0.25.0"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": "https://github.com/vish288/move-prop-types.git"
}
